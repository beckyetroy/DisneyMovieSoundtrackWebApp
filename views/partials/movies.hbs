{{#each playlists}}
  <section class="ui segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <p> Total Duration: {{duration}} </p>
    <a href="/playlist/{{id}}"><i class = "open folder icon"></i></a>
    <a href="/dashboard/{{id}}/deleteplaylist"> <i class = "trash icon delplaylist"></i></a>
  </section>
{{/each}}