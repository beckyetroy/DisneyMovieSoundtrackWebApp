<table class="ui stackable yellow striped table">
  <thead>
    <tr>
      <th>Song</th>
      <th>Sung by</th>
      <th>Genre</th>
      <th> </th>
      <th> </th>
    </tr>
  </thead>
  <tbody>
    {{#each movie.tracks}}
      <tr>
        <td>
          {{title}}
        </td>
        <td>
          {{singer}}
        </td>
        <td>
          {{genre}}
        </td>
        <td class = "listen">
          <a href ="{{youtube}}" target="_blank">Listen now</a>
        </td>
        <td class = "delete">
          <a href="/movie/{{../movie.id}}/deletetrack/{{id}}" class="ui icon button deltrack"> <i class="icon trash"></i></a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>