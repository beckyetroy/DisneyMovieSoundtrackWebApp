{{> menu}}

<section class="ui grid">
        
        <div class = "ui two wide stackable column">
            <img class = "ui square image" src = "https://cdn.glitch.com/d2abf87c-5b55-4b85-a313-c94daaba6a2c%2F00-story-little-mermaid.jpg?v=1615742267632" id = "Left1">
            <img class = "ui square image" src = "https://cdn.glitch.com/d2abf87c-5b55-4b85-a313-c94daaba6a2c%2Faladdin-2000.jpg?v=1615742280013" id = "Left2">
            <img class = "ui square image" src = "https://cdn.glitch.com/d2abf87c-5b55-4b85-a313-c94daaba6a2c%2Fdisney-e2-80-a2pixar-s-coco-celebrates-a-host-of-brands-in-far-reaching-promotional-campaign.jpg?v=1615742298001" id = "Left3">
            <img class = "ui square image" src = "https://cdn.glitch.com/d2abf87c-5b55-4b85-a313-c94daaba6a2c%2FProfile_-_Lady.jpg?v=1615742601356" id = "Left4">
        </div>

      <div class="ui twelve wide column">
        <h1>Disney Movie Soundtracks 101</h1>
        <h1 class="ui header">
          Hi, {{fullname}}!
        </h1>
        <div class = "stats">Your stats so far: 
          <br>
        <div class="ui big blue statistic">
        <div class="value">
            {{totalMovies}}
        </div>
        <div class="label">
            Movies
        </div>
    </div>
    <div class="ui big blue statistic">
        <div class="value">
            {{totalTracks}}
        </div>
        <div class="label">
            Tracks
        </div>
    </div>
    <div class="ui big blue statistic">
        <div class="value">
            {{avgMovies}}
        </div>
        <div class="label">
            Movies <br>per User
        </div>
    </div>
    <div class="ui big blue statistic">
        <div class="value">
            {{avgTracks}}
        </div>
        <div class="label">
            Tracks <br>per User
        </div>
    </div>
    <br><br>
    <div class="ui big blue horizontal statistic">
        <div class="text value">
            {{bigContributer}} <img src="{{bigProfilePic}}" class="ui circular inline image">
        </div>
        <div class="label" id = "bigContributer">
            : Our biggest contributor(s)
        </div>
    </div>
          <br>
    <div class="ui big blue horizontal statistic">
        <div class="text value">
            {{smallContributer}} <img src="{{smallProfilePic}}" class="ui circular inline image">
        </div>
        <div class="label" id = "smallContributer">
            : Our smallest contributor(s)
        </div>
    </div>
          
        </div>
      
        
      </div>

        <div class = "ui two wide stackable column">
            <img class = "ui square image" src = "https://cdn.glitch.com/d2abf87c-5b55-4b85-a313-c94daaba6a2c%2F239649989.jpg.gallery.jpg?v=1615674618968" id = "Right1">
